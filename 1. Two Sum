class Solution {
    public int[] twoSum(int[] nums, int target) {     
    int length= nums.length;
    int result[]=new int[2];
    
    for(int i=0;i<=length-1;i++)
    {
        for(int j= i+1;j<=length-1;j++)
        {
            if (nums[i]+nums[j]==target)
           { 
            result[0]=i;
            result[1]=j;
           }  
        }
    }
    return result;
    
        
    }
}
